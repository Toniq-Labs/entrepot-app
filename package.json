    "scripts": {
        "build": "tsc && ts-node scripts/build.ts pre && react-app-rewired build && ts-node scripts/build.ts post",
        "eject": "react-app-rewired eject",
        "format": "prettier --color --list-different --write \"./**/*.+(css|html|js|jsx|tsx|json|md|toml|ts|yaml|yml)\" --config \"./.prettierrc.js\"",
        "postinstall": "ts-node scripts/build.ts install",
        "preview": "npm run build && serve -s build",
        "start": "react-app-rewired start",
        "start:clean": "rm -rf node_modules/.cache && npm i && react-app-rewired start",
        "test": "virmator test-web",
        "test:all": "npm run test:format && npm run test:spelling && npm run test:types && npm run test:lint && npm run test",
        "test:format": "prettier --color --check \"./**/*.+(css|html|js|jsx|tsx|json|md|toml|ts|yaml|yml)\" --config \"./.prettierrc.js\"",
        "test:lint": "eslint --config ./.eslintrc.cjs \"src/**/*.ts\"",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "browserslist": {
        "production": [
            "last 2 chrome version",
