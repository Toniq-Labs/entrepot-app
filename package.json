    "private": true,
    "scripts": {
        "build": "npm test && react-app-rewired build",
        "eject": "react-app-rewired eject",
        "format": "prettier \"./**/*.+(js|json|ts|tsx|html|css|md|toml|yml|yaml)\" --write",
        "serve": "react-app-rewired build && serve -s build",
